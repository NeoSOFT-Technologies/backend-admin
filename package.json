{
  "name": "console",
  "version": "1.0.0",
  "homepage": "/",
  "author": "Santosh Shinde",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "start:prod": "serve -s build -p 4004",
    "build": "react-scripts build",
    "clean": "rimraf {bin,build}",
    "clean:cdn": "rimraf build/global/css",
    "build:prod": "npm run build -- --profile",
    "test": "rimraf src/__snapshots__ && react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,scss}\"",
    "test:coverage": "npm test -- --coverage",
    "lint": "eslint . --ext js,jsx,mjs,ts,tsx ",
    "lint:fix": "eslint . --ext js,jsx,mjs,ts,tsx --fix",
    "lint:quiet": "eslint . --ext js,jsx,mjs,ts,tsx --quiet",
    "prepare": "husky install",
    "precommit": "npm run lint:fix && npm run format",
    "prepush": "npm run lint"
  },
  "dependencies": {
    "@mdi/font": "^6.5.95",
    "@reduxjs/toolkit": "^1.8.0",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.1",
    "@types/react-bootstrap": "^0.32.29",
    "@types/react-dom": "^18.0.0",
    "@types/react-redux": "^7.1.23",
    "axios": "^0.26.0",
    "axios-mock-adapter": "^1.20.0",
    "bootstrap": "^4.3.1",
    "compass-mixins": "^0.12.10",
    "gridjs": "^5.0.2",
    "gridjs-react": "^5.0.2",
    "jquery": "^3.6.0",
    "moment": "^2.29.3",
    "react": "^17.0.2",
    "react-bootstrap": "^2.3.0",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-image": "^4.0.3",
    "react-redux": "^8.0.0",
    "react-router-dom": "^6.2.2",
    "react-scripts": "5.0.1",
    "react-toastify": "^8.2.0",
    "redux-mock-store": "^1.5.4",
    "typescript": "~4.6.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^14.0.4",
    "@types/jest": "^27.4.1",
    "@types/jquery": "^3.5.14",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux-mock-store": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-filename-rules": "^1.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-unicorn": "^42.0.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "sass": "^1.49.9",
    "serve": "^13.0.2"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "src/serviceWorker.ts",
      "setupTests.ts",
      "react-app-env.d.ts",
      "__snapshots__/*",
      "index.ts",
      "src/types/landing-page.ts",
      "services",
      "AdminDashboard.tsx",
      "src/routes/gateway",
      "src/routes/saas"
    ],
    "collectCoverageFrom": [
      "src/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NeoSOFT-Technologies/console.git"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
